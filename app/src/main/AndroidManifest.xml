<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.siya.mylibraryrealworld">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/siya_library"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/siya_library"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WebsiteActivity"></activity>
        <activity android:name=".FavoriteBooksActivity"
            android:label="@string/FavoriteBooksActivity"/>
        <activity android:name=".CurrentlyReadingActivity"
            android:label="@string/CurrentlyReadingActivity"/>
        <activity android:name=".WantToReadActivity"
            android:label="@string/WantToReadActivity"/>
        <activity android:name=".AlreadyReadBookActivity"
            android:label="@string/AlreadyReadBookActivity"/>
        <activity android:name=".BookActivity"
            android:label="@string/BookActivity"/>
        <activity android:name=".SeeAllBooksActivity"
            android:label="@string/SeeAllBooksActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>